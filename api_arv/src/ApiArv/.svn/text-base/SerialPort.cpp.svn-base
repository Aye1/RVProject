#include "SerialPort.h"
#include <iostream>

namespace arv {

char* getSystemName(SerialPort port) {
    switch (port) {
        case PORT0: return "/dev/ttyS0";
        case PORT1: return "/dev/ttyS1";
        case PORT2: return "/dev/ttyS2";
        case PORT3: return "/dev/ttyS3";
        };
    return NULL;
    }

}

